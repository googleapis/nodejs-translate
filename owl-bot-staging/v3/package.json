{
  "name": "@google-cloud/translate",
  "version": "0.1.0",
  "description": "Translation client for Node.js",
  "repository": "googleapis/nodejs-translation",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "main": "build/src/index.js",
  "files": [
    "build/src",
    "build/protos"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google translation",
    "translation",
    "translation service"
  ],
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "fix": "gts fix",
    "lint": "gts check",
    "prepare": "npm run compile-protos && npm run compile",
    "system-test": "c8 mocha build/system-test",
    "test": "c8 mocha build/test"
  },
  "dependencies": {
    "google-gax": "^2.12.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.43",
    "@types/sinon": "^10.0.0",
    "c8": "^7.7.1",
    "gts": "^3.1.0",
    "jsdoc": "^3.6.6",
    "jsdoc-fresh": "^1.0.2",
    "jsdoc-region-tag": "^1.0.6",
    "linkinator": "^2.13.6",
    "mocha": "^8.3.2",
    "null-loader": "^4.0.1",
    "pack-n-play": "^1.0.0-2",
    "sinon": "^10.0.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "engines": {
    "node": ">=v10.24.0"
  }
}
