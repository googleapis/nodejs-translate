{
  "name": "@google-cloud/translate",
  "description": "Cloud Translation API Client Library for Node.js",
  "version": "4.2.1",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=8.10.0"
  },
  "repository": "googleapis/nodejs-translate",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src",
    "build/protos",
    "!build/src/**/*.map"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google translate",
    "translate"
  ],
  "scripts": {
    "docs": "jsdoc -c .jsdoc.js",
    "predocs": "npm run compile",
    "lint": "npm run check && eslint '**/*.js'",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "system-test": "mocha build/system-test --timeout 600000",
    "test": "c8 mocha build/test",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p . && npm run copy-js",
    "copy-js": "cp -r src/v3beta1 build/src/ && cp -r protos build/ && cp test/*.js build/test",
    "fix": "gts fix && eslint --fix '**/*.js'",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "presystem-test": "npm run compile",
    "docs-test": "linkinator docs",
    "predocs-test": "npm run docs"
  },
  "dependencies": {
    "@google-cloud/common": "^2.0.0",
    "@google-cloud/promisify": "^1.0.0",
    "arrify": "^2.0.0",
    "extend": "^3.0.2",
    "google-gax": "^1.7.5",
    "is": "^3.2.1",
    "is-html": "^2.0.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/extend": "^3.0.0",
    "@types/is": "0.0.21",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.7",
    "@types/proxyquire": "^1.3.28",
    "@types/request": "^2.47.1",
    "c8": "^6.0.0",
    "codecov": "^3.0.2",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gts": "^1.0.0",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.6.2",
    "jsdoc-fresh": "^1.0.1",
    "linkinator": "^1.5.0",
    "mocha": "^6.1.4",
    "power-assert": "^1.6.0",
    "prettier": "^1.13.5",
    "proxyquire": "^2.0.1",
    "source-map-support": "^0.5.6",
    "typescript": "~3.6.0"
  }
}
